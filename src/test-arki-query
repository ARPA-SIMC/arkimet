#!/bin/sh -e

test -z "$ARGS" || exit 0

BINDIR=`dirname $0`

. $BINDIR/testlib.sh

setup
trap cleanup EXIT

importtest

echo -n "Testing arki-query"

# Try passing files to the postprocessor
runtest "test `arki-query --postproc=checkfiles '' test200 --postproc-data=/dev/null` = /dev/null"

testtotals

exit $BADCOUNT
